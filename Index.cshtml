@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    @foreach (var item in @ViewBag.listaClientes)
{
   <tr>
   <th> scope="row">@item.Id</th>
   <td>@item.Nome</td>
   <td>@item.Cpf</td>
   <td>@item.Email</td>
   <td>@item.Paciente</td>
  </tr>
}
@foreach (var item in @ViewBag.listaFornecedores)
{
   <tr>
   <th scope="row">@item.Id</th>
   <td>@item.Nome</td>
   <td>@item.Cnpj</td>
   <td>@item.Email</td>
   </tr>
}
</div>
